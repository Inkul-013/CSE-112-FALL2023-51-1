#include <iostream>
using namespace std;
template <typename T>
T greatest(T a, T b, T c)
{
    return max(max(a, b), c);
}
int main()
{
    int a, b, c;
    cin >> a >> b >> c;
    cout << greatest(a, b, c) << endl;
    char x, y, z;
    cin >> x >> y >> z;
    cout << greatest(x, y, z) << endl;
    float p, q, r;
    cin >> p >> q >> r;
    cout << greatest(p, q, r) << endl;
    return 0;
}
